{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.36",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.7.1",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "date-fns": "^2.8.1",
    "formik": "^2.1.3",
    "formik-material-ui": "^2.0.0-alpha.3",
    "formik-material-ui-pickers": "^0.0.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sinon": "^8.1.1",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:mock": "node src/server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "yarn run test --watchAll=false",
    "test:coverage": "yarn run test:once -- --coverage",
    "lint": "eslint src",
    "check": "yarn run lint && yarn run test:coverage",
    "clean": "rm -rf node_modules && yarn install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/serviceWorker.js",
      "!src/server/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 15,
        "functions": 15,
        "lines": 15,
        "statements": 15
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "husky": "^3.0.9",
    "prettier": "^1.19.1"
  }
}
